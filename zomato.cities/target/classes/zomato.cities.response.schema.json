{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "location_suggestions",
    "status",
    "has_more",
    "has_total"
  ],
  "properties": {
    "location_suggestions": {
      "$id": "#/properties/location_suggestions",
      "type": "array",
      "title": "The Location_suggestions Schema",
      "items": {
        "$id": "#/properties/location_suggestions/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "id",
          "name",
          "country_id",
          "country_name",
          "country_flag_url",
          "should_experiment_with",
          "discovery_enabled",
          "has_new_ad_format",
          "is_state",
          "state_id",
          "state_name",
          "state_code"
        ],
        "properties": {
          "id": {
            "$id": "#/properties/location_suggestions/items/properties/id",
            "type": "integer",
            "title": "The Id Schema",
            "default": 0,
            "examples": [
              91
            ]
          },
          "name": {
            "$id": "#/properties/location_suggestions/items/properties/name",
            "type": "string",
            "title": "The Name Schema",
            "default": "",
            "examples": [
              "Dublin"
            ],
            "pattern": "^(.*)$"
          },
          "country_id": {
            "$id": "#/properties/location_suggestions/items/properties/country_id",
            "type": "integer",
            "title": "The Country_id Schema",
            "default": 0,
            "examples": [
              97
            ]
          },
          "country_name": {
            "$id": "#/properties/location_suggestions/items/properties/country_name",
            "type": "string",
            "title": "The Country_name Schema",
            "default": "",
            "examples": [
              "Ireland"
            ],
            "pattern": "^(.*)$"
          },
          "country_flag_url": {
            "$id": "#/properties/location_suggestions/items/properties/country_flag_url",
            "type": "string",
            "title": "The Country_flag_url Schema",
            "default": "",
            "examples": [
              "https://b.zmtcdn.com/images/countries/flags/country_97.png"
            ],
            "pattern": "^(.*)$"
          },
          "should_experiment_with": {
            "$id": "#/properties/location_suggestions/items/properties/should_experiment_with",
            "type": "integer",
            "title": "The Should_experiment_with Schema",
            "default": 0,
            "examples": [
              0
            ]
          },
          "discovery_enabled": {
            "$id": "#/properties/location_suggestions/items/properties/discovery_enabled",
            "type": "integer",
            "title": "The Discovery_enabled Schema",
            "default": 0,
            "examples": [
              0
            ]
          },
          "has_new_ad_format": {
            "$id": "#/properties/location_suggestions/items/properties/has_new_ad_format",
            "type": "integer",
            "title": "The Has_new_ad_format Schema",
            "default": 0,
            "examples": [
              1
            ]
          },
          "is_state": {
            "$id": "#/properties/location_suggestions/items/properties/is_state",
            "type": "integer",
            "title": "The Is_state Schema",
            "default": 0,
            "examples": [
              0
            ]
          },
          "state_id": {
            "$id": "#/properties/location_suggestions/items/properties/state_id",
            "type": "integer",
            "title": "The State_id Schema",
            "default": 0,
            "examples": [
              0
            ]
          },
          "state_name": {
            "$id": "#/properties/location_suggestions/items/properties/state_name",
            "type": "string",
            "title": "The State_name Schema",
            "default": "",
            "examples": [
              ""
            ],
            "pattern": "^(.*)$"
          },
          "state_code": {
            "$id": "#/properties/location_suggestions/items/properties/state_code",
            "type": "string",
            "title": "The State_code Schema",
            "default": "",
            "examples": [
              ""
            ],
            "pattern": "^(.*)$"
          }
        }
      }
    },
    "status": {
      "$id": "#/properties/status",
      "type": "string",
      "title": "The Status Schema",
      "default": "",
      "examples": [
        "success"
      ],
      "pattern": "^(.*)$"
    },
    "has_more": {
      "$id": "#/properties/has_more",
      "type": "integer",
      "title": "The Has_more Schema",
      "default": 0,
      "examples": [
        0
      ]
    },
    "has_total": {
      "$id": "#/properties/has_total",
      "type": "integer",
      "title": "The Has_total Schema",
      "default": 0,
      "examples": [
        0
      ]
    }
  }
}